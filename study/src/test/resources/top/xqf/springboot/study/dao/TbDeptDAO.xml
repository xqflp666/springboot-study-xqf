<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.xqf.springboot.study.dao.TbDeptDAO">

 <select id="CX" parameterType="TbDept" resultType="TbDept" >
  select * from tb_dept
 </select>

 <insert id="add" parameterType="TbDept">
  insert into tb_dept(dept_name,dept_info) values(#{deptName},#{deptInfo})
 </insert>

</mapper>